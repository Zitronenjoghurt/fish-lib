{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[0-9]+$": {
      "type": "object",
      "required": [
        "name",
        "default_properties"
      ],
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of the item"
        },
        "default_properties": {
          "type": "object",
          "oneOf": [
            {
              "type": "object",
              "required": [
                "type"
              ],
              "properties": {
                "type": {
                  "type": "string",
                  "enum": [
                    "None"
                  ]
                }
              },
              "additionalProperties": false
            },
            {
              "type": "object",
              "required": [
                "type",
                "times_used"
              ],
              "properties": {
                "type": {
                  "type": "string",
                  "enum": [
                    "Rod"
                  ]
                },
                "times_used": {
                  "type": "integer",
                  "minimum": 0
                }
              },
              "additionalProperties": false
            }
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}